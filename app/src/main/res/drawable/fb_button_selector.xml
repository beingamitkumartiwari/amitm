<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item  android:state_pressed="true">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#174CA5"/>
            <corners
                android:radius="1dp"/>
            <stroke
                android:color="#9e303bb1"
                android:width="7dp"/>
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape
            android:shape="rectangle">
            <solid
                android:color="#174CA5"/>
            <corners
                android:radius="1dp"/>
            <stroke
                android:color="#003441c8"
                android:width="7dp"/>
        </shape>
    </item>
</selector>